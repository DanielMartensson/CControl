#include "../../Headers/Functions.h"

/*
 * Compute the mean of vector a
 * a[L] Vector with values
 * L = Length of vector a
 */
float mean(float* a, uint32_t L) {
	float s = 0;
	for(uint32_t i = 0; i < L; i++)
		s += *(a + i);
	return s/((float) L);
}
